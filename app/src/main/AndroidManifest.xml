<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wenitech.cashdaily">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.PerfilClienteActivity"/>
        <activity android:name=".ui.Main.ActivityClientes.CreditoVacioActivity" />
        <activity android:name=".ui.Main.ActivityProductosYServicios.ProductosActivity" />
        <activity android:name=".ui.Main.ActivityGastos.GastosActivity" />
        <activity android:name=".ui.Main.ActivityEstadisticas.EstadisticasActivity" />
        <activity android:name=".ui.Main.ActivityCobradores.CobradorActivity" />
        <activity android:name=".ui.Main.ActivitySincronizacion.SincronizacionActivity" />
        <activity
            android:name=".ActivitysNavigationDrawer.ClassForUtilCode.PerfilUsuarioActivity"
            android:parentActivityName=".ui.MainNavActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.MainNavActivity" />
        </activity>
        <activity android:name=".ui.Main.ActivityClientes.ActivityNuevoCredito.NewCreditActivity" />
        <activity android:name=".ui.Main.ActivityClientes.ActivityNuevoCliente.NewClientActivity" />
        <activity android:name=".ui.Main.ActivityClientes.ActivityCreditoCliente.CreditoClienteActivity" />
        <activity
            android:name=".ui.MainNavActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.login.LoginActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>