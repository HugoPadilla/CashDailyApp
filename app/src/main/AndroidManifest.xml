<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wenitech.cashdaily">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NewCreditActivity.NewCreditActivity"></activity>
        <activity android:name=".SingInActivity.SingInActivity" />
        <activity android:name=".LoginActivity.LoginActivity" />
        <activity android:name=".GastoDetail.GastoDetailActivity" />
        <activity android:name=".NewClient.NewClientActivity" />
        <activity android:name=".ClienteDetail.ClienteDetailActivity" />
        <activity android:name=".NewGastoActivity.NewGastoActivity" />
        <activity android:name=".MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewClientActivity.NewClientActivity"
            android:parentActivityName=".MainActivity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity.MainActivity" />
        </activity>
    </application>

</manifest>